{
  "name": "@wingriders/wallet-gateway",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "bun gql-gen && tsc -b && vite build",
    "preview": "vite preview",
    "check": "bun gql-gen && bun biome check src",
    "fix": "bun gql-gen && bun run check --write",
    "test": "bun gql-gen && bun test",
    "test:watch": "bun gql-gen && bun test --watch",
    "gql-gen": "graphql-codegen --config codegen.ts -r dotenv/config"
  },
  "dependencies": {
    "@apollo/client": "^3.11.8",
    "@cardano-sdk/crypto": "^0.1.30",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@emurgo/cardano-message-signing-asmjs": "^1.0.1",
    "@fontsource/roboto": "^5.1.0",
    "@mui/icons-material": "^6.1.0",
    "@mui/lab": "^6.0.0-beta.10",
    "@mui/material": "^6.1.0",
    "@tanstack/react-router": "^1.57.18",
    "@wingriders/cab": "^2.0.30",
    "@wingriders/wallet-common": "workspace:*",
    "borc": "^3.0.0",
    "date-fns": "^4.1.0",
    "graphql": "^16.9.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "zod": "^3.23.8",
    "zustand": "^5.0.0-rc.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.1",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/client-preset": "4.3.3",
    "@happy-dom/global-registrator": "^15.7.4",
    "@rollup/plugin-inject": "^5.0.5",
    "@tanstack/router-plugin": "^1.57.15",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "dotenv": "^16.4.5",
    "globals": "^15.9.0",
    "ts-pattern": "^5.3.1",
    "type-fest": "^4.26.1",
    "typescript": "^5.6.2",
    "vite": "^5.4.1",
    "vite-plugin-node-polyfills": "^0.22.0",
    "vite-plugin-wasm": "^3.3.0"
  }
}
